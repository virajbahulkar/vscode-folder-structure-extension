{
  "name": "my-folder-structure-generator",
  "displayName": "Folder Structure Creator",
  "description": "Create a folder/file structure from a text file with indented hierarchy.",
  "version": "1.0.2",
  "publisher": "VirajBahulkar",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.createStructure"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createStructure",
        "title": "Generate Folder Structure"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p .",
    "watch": "tsc -watch -p .",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.0.0",
    "vsce": "^2.15.0",
    "vscode-test": "^1.6.0"
  }
}
